{ echo "$as_me:$LINENO: result: $GNUSTEP_SYSTEM_TOOLS" >&5
echo "${ECHO_T}$GNUSTEP_SYSTEM_TOOLS" >&6; }
{ echo "$as_me:$LINENO: checking for System Admin Tools directory" >&5
