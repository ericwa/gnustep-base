echo $ECHO_N "checking for user directory... $ECHO_C" >&6; }
{ echo "$as_me:$LINENO: result: $GNUSTEP_USER_DIR" >&5
echo "${ECHO_T}$GNUSTEP_USER_DIR" >&6; }
{ echo "$as_me:$LINENO: checking for user defaults directory" >&5
